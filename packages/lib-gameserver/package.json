{
  "name": "@takaro/gameserver",
  "version": "0.0.1",
  "description": "Handles abstraction between Takaro and game servers",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "start:dev": "swc -w ./src -d ./dist",
    "build": "swc ./src -d ./dist",
    "test": "npm run test:unit --if-present && npm run test:integration --if-present",
    "test:unit": "ts-mocha -p ./tsconfig.json src/**/*.unit.test.ts",
    "test:integration": "ts-mocha -p ./tsconfig.json --file ../test/src/waitUntilReady.ts src/**/*.integration.test.ts --exit",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@takaro/test": "0.0.1",
    "@types/eventsource": "^1.1.9"
  }
}
