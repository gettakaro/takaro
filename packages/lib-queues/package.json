{
  "name": "@takaro/queues",
  "version": "0.0.1",
  "description": "An opinionated BullMQ queues handler",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "start:dev": "tsc --watch --preserveWatchOutput -p ./tsconfig.build.json",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "npm run test:unit --if-present && npm run test:integration --if-present",
    "test:unit": "MOCHA_FILE=../../reports/mocha/lib-queues-unit.xml ts-mocha -p ./tsconfig.json src/**/*.unit.test.ts",
    "test:integration": "MOCHA_FILE=../../reports/mocha/lib-queues-integration.xml ts-mocha -p ./tsconfig.json --file ../test/src/waitUntilReady.ts src/**/*.integration.test.ts --exit",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@takaro/test": "0.0.1"
  }
}