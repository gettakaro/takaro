{
  "name": "@takaro/util",
  "version": "0.0.1",
  "description": "An opinionated logger",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "scripts": {
    "start:dev": "tsc --watch --preserveWatchOutput -p ./tsconfig.build.json",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "npm run test:unit --if-present && npm run test:integration --if-present",
    "test:unit": "tsc -p ./tsconfig.json && mocha --config ../../.mocharc.js dist/**/*.unit.test.js",
    "test:integration": "tsc -p ./tsconfig.json && mocha --config ../../.mocharc.js dist/**/*.integration.test.js --exit",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@takaro/config": "0.0.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@takaro/test": "0.0.1"
  }
}